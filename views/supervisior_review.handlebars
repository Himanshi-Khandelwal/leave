<div>
<br>
	<table>
<thead>
    <tr>
      <th scope="col">From Person</th>
      <th scope="col">From Date</th>
      <th scope="col">To Date</th>
      <th scope="col">Status</th>
       <th scope="col">Changes</th>
       <th scope="col">Reason</th>
    </tr>
  </thead>
  <tbody>
    {{#each applis}}
	<tr>
		<td>{{this.fromPerson}}</td>
		<td>{{this.from}}</td>
		<td>{{this.to}}</td>
		<td width="150%">
			<button class="btn btn-info">Accept</button> <button class="btn btn-info">Reject</button> 
		</td>
		<td>{{this.reason}}</td>
	</tr>	
	{{/each}}
  </tbody>
</table>
</div>


